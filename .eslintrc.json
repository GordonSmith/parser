{
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "semi": 2,
    "no-cond-assign": 0,
    "no-process-env": 2,
    "no-var": 2,
    "quotes": ["error", "double", {"allowTemplateLiterals": true}]
  }
}
